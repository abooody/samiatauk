RailsBaseURI /

RewriteEngine On 
RewriteCond %{HTTP_HOST} ^www.samiatauk.com.br$ [NC]
RewriteRule ^(.*)$ http://samiatauk.com.br/$1 [R=301,L]

ExpiresActive On
ExpiresDefault "access plus 10 minutes"
ExpiresByType text/html "access plus 4 minutes"

AddOutputFilterByType DEFLATE text/html text/plain text/xml application/javascript text/css